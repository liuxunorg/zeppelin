{{ HADOOP_HOME }}/bin/yarn jar {{ HADOOP_YARN_SUBMARINE_JAR }} \
  job run \
  --name {{ JOB_NAME }} \
  --env DOCKER_JAVA_HOME=/opt/java \
  --env DOCKER_HADOOP_HDFS_HOME=/hadoop-3.1.0 \
  --env YARN_CONTAINER_RUNTIME_DOCKER_CONTAINER_NETWORK={{ DOCKER_CONTAINER_NETWORK }} \
  --input_path {{ INPUT_PATH }} \
  --checkpoint_path {{ CHECKPOINT_PATH }} \
  --num_ps {{ PARAMETER_SERVICES_NUM }} \
  --ps_resources memory={{ PARAMETER_SERVICES_MEMORY }},vcores={{ PARAMETER_SERVICES_CPU }},gpu={{ PARAMETER_SERVICES_GPU }} \
  --ps_launch_cmd "{{ PS_LAUNCH_CMD }}" \
  --ps_docker_image {{ PARAMETER_SERVICES_DOCKER_IMAGE }} \
  --worker_docker_image {{ WORKER_SERVICES_DOCKER_IMAGE }} \
  --worker_resources memory={{ WORKER_SERVICES_MEMORY }},vcores={{ WORKER_SERVICES_CPU }},gpu={{ WORKER_SERVICES_GPU }} \
  --verbose \
  --num_workers {{ WORKER_SERVICES_NUM }} \
  --worker_launch_cmd "{{ WORKER_LAUNCH_CMD }}" \